	<Component BuildTime="0x002A910F5730D901" Guid="{4E2D44D7-85F9-4B2D-BFB0-682C6610908A}" Min_Compliant_Version="8000" Name="NWBC Set Variable PreUninstall" Order="PostInstall" Version="8000">
		<Action RunAt="Uninstall" Type="Script">
			<Src><![CDATA[
NwEngine.Context.Log.Write "Checking other installed NWBC releases."
		
		Dim RegSapSharedKey, RegVal, releases, release, highestRelease
		RegSapSharedKey = "HKLM\SOFTWARE\SAP\SAP Shared\"
		releases = Array("SAP_NWBC50", "SAP_NWBC60", "SAP_NWBC65", "SAP_NWBC70", "SAP_NWBC770")
		RegVal = "NONE"
		highestRelease = "NONE"
		
		For Each release In releases
			NwEngine.Context.Log.Write "Checking: " & release
			If NwEngine.Shell.RegKeyExist(RegSapSharedKey & release) Then
				NwEngine.Context.Log.Write RegSapSharedKey & release & " exists."
				If NwEngine.Shell.RegValueExist(RegSapSharedKey & release & "\SAPDestDir") Then
					RegVal = NwEngine.Shell.GetRegValue(RegSapSharedKey & release & "\SAPDestDir")
					highestRelease = release
					NwEngine.Context.Log.Write "Registry value of " & RegSapSharedKey & release & "\SAPDestDir is: " & RegVal
				End if
			Else
				NwEngine.Context.Log.Write RegSapSharedKey & release & " does not exist."
			End if
		Next
		
		NwEngine.Variables.InsertVariable "SapNWBCHighestOtherRelease", highestRelease
		NwEngine.Context.Log.Write "Set SapNWBCHighestOtherRelease to: " & NwEngine.Variables.GetValue("SapNWBCHighestOtherRelease")
		NwEngine.Variables.InsertVariable "SapNWBCHighestOtherReleaseDestDir", RegVal
		NwEngine.Context.Log.Write "Set SapNWBCHighestOtherReleaseDestDir to: " & NwEngine.Variables.GetValue("SapNWBCHighestOtherReleaseDestDir")
			]]></Src>
		</Action>
	</Component>
